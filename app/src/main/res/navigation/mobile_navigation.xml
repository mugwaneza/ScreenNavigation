<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.screennavigation.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.screennavigation.ui.clients.ClientsFragment"
        android:label="@string/client_id"
        tools:layout="@layout/fragment_clients" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.screennavigation.ui.history.HistoryFragment"
        android:label="@string/history_id"
        tools:layout="@layout/fragment_history"/>

    <fragment
        android:id="@+id/nav_manage"
        android:name="com.example.screennavigation.ui.manage.ManageFragment"
        android:label="@string/manageid"
        tools:layout="@layout/fragment_manage"/>



</navigation>